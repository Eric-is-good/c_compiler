    int main(int args[]){
        int a;
        a = 6;
        a = a+10;
        if(a>9){
            return a;
        }else{
            int b;
            b = a * a;
        }
    }

antlr4 -package paser -visitor miniC.g4
grun miniC program -gui
javac -cp /usr/local/lib/antlr-4.9-complete.jar miniC*.java